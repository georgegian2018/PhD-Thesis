// Place your settings in this file to overwrite default and user settings.
{
    "editor.wordWrap": "bounded",
    "latex-workshop.latex.toolchain": [
        {
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "thesis"
            ]
        }
    ],
    "editor.wordWrapColumn": 80
}
